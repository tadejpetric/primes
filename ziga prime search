#include <iostream>
using namespace std;
 
int main()
{
  int n, temp=0;
  cout << "Vpisi pozitivno stevilo: ";
  cin >> n;
  for(int i=2;i<=n/2;++i)
      if(n%i==0)
      {
          temp=1;
          break;
      }
  if (temp==0)
      cout << "Je pra stevilo";
  else
      cout << "Ni pra stevilo";
  return 0;
}
